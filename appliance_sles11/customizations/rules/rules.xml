<?xml version="1.0"?>
<!DOCTYPE autoinstall SYSTEM "/usr/share/autoinstall/dtd/rules.dtd">
<autoinstall xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
  <rules config:type="list">
   <rule>
    <disksize>
     <match>/dev/sdb 10000</match>
     <match_type>greater</match_type>
    </disksize>
    <result>
     <profile>autoinst.xml</profile>
     <continue config:type="boolean">false</continue>
    </result>
   </rule>
  <rule>
   <disksize>
    <match>/dev/sda 10000</match>
    <match_type>greater</match_type>
   </disksize>
   <result>
    <profile>autoinst_legacy.xml</profile>
    <continue config:type="boolean">false</continue>
   </result>
  </rule>
 </rules>
</autoinstall>
